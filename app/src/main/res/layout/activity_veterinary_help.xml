<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".VeterinaryHelpActivity"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/VeterinaryHelpRelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentTop="true"
        android:background="@color/grey100"
        >

        <ImageView
            android:id="@+id/VeterinaryHelpAppLogo"
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/ic_applogofit"
            tools:ignore="ContentDescription" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/VeterinaryHelpLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/VeterinaryHelpRelativeLayout"
        android:orientation="horizontal"
        android:gravity="center"
        >

        <ImageView
            android:id="@+id/Maphelper"
            android:layout_width="150dp"
            android:layout_height="110dp"
            android:src="@drawable/maplocation"
            tools:ignore="ContentDescription"
            />

        <ImageView
            android:id="@+id/petcare"
            android:layout_width="150dp"
            android:layout_height="110dp"
            android:src="@drawable/petcare"
            tools:ignore="ContentDescription"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/VeterinaryHelpLinearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VeterinaryHelpLinearLayout"
        android:orientation="horizontal"
        android:gravity="center"
        >

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:fontFamily="@font/hind_vadodara_medium"
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:text="Nearby Help"
            />

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:fontFamily="@font/hind_vadodara_medium"
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Pet Care"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/VeterinaryHelpLinearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VeterinaryHelpLinearLayout2"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginTop="15dp"
        >

        <ImageView
            android:id="@+id/Vaccines"
            android:layout_width="150dp"
            android:layout_height="110dp"
            android:src="@drawable/medicinestores"
            tools:ignore="ContentDescription"
            />

        <ImageView
            android:id="@+id/mypet"
            android:layout_width="150dp"
            android:layout_height="110dp"
            android:src="@drawable/rabbit"
            tools:ignore="ContentDescription"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/VeterinaryHelpLinearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VeterinaryHelpLinearLayout3"
        android:orientation="horizontal"
        android:gravity="center"
        >

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:fontFamily="@font/hind_vadodara_medium"
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Vaccines"
            />

        <TextView
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:fontFamily="@font/hind_vadodara_medium"
            android:gravity="center_horizontal"
            android:textAlignment="center"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Details"
            />
    </LinearLayout>


</LinearLayout>